
import java.util.*;
import java.lang.*;
import java.io.*;
public class GuessNo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		
		int My_no=(int)(Math.random()*100);
		
		int user_no=0;
		
		do {
			
			System.out.println("Guess The Number ");
			user_no=scan.nextInt();
			
			if(user_no == My_no) {
				
				System.out.println("Correct your Number");
				
				break;
			}
			
			else if(user_no > My_no) {
				
				System.out.println("Higher then the Generated Number");
			}
			
			else {
				
				System.out.println("Lower then the Generated Number ");
			}
		}
			while(user_no >=0) ;
				
				System.out.print("My Number was : ");
				
				System.out.println(My_no);
			
	}
}
